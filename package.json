{
    "name": "exaltio-mandate-frontend",
    "description": "Frontend for temporary Exaltio mandate system.",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "private": true,
    "author": "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)",
    "repository": "git@ssh.danielhuisman.io:exaltio/exaltio-mandate-frontend.git",
    "keywords": [
        "exaltio",
        "mandate",
        "frontend"
    ],
    "scripts": {
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run lint && yarn run build:parcel",
        "build:parcel": "parcel build public/index.html --public-url / --no-source-maps ",
        "dev": "parcel public/index.html",
        "codegen": "apollo client:codegen --target typescript --endpoint https://mandate.exaltio.nl/api/v1 --outputFlat src/types/generatedTypes.ts",
        "codegen:dev": "apollo client:codegen --target typescript --endpoint http://localhost:5000/graphql --outputFlat src/types/generatedTypes.ts --watch"
    },
    "dependencies": {
        "@types/helmet": "^0.0.43",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.5",
        "@types/react-google-recaptcha": "^1.0.0",
        "@types/react-helmet": "^5.0.8",
        "@types/react-router-dom": "^4.3.4",
        "@types/yup": "^0.26.22",
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link-http": "^1.5.15",
        "apollo-upload-client": "^11.0.0",
        "formik": "^1.5.8",
        "graphql": "^14.4.2",
        "i18next": "^17.0.6",
        "i18next-browser-languagedetector": "^3.0.1",
        "moment": "^2.24.0",
        "qs": "^6.7.0",
        "react": "^16.8.6",
        "react-apollo": "^2.5.8",
        "react-dom": "^16.8.6",
        "react-error-boundary": "^1.2.5",
        "react-google-recaptcha": "^1.1.0",
        "react-helmet": "^5.2.1",
        "react-i18next": "^10.11.4",
        "react-piwik": "^1.6.0",
        "react-router-dom": "^5.0.1",
        "react-router-query-params-hook": "^1.0.4",
        "semantic-ui-react": "^0.87.3",
        "yup": "^0.27.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "apollo": "^2.16.2",
        "babel-plugin-i18next-extract": "^0.2.2",
        "graphql-tag": "^2.10.0",
        "parcel-bundler": "^1.10.3",
        "postcss-modules": "^1.4.1",
        "rimraf": "^2.6.2",
        "tslint": "^5.18.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.5.3"
    }
}
