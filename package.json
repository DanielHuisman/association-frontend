{
    "name": "exaltio-mandate-frontend",
    "description": "Frontend for temporary Exaltio mandate system.",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "private": true,
    "author": "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)",
    "repository": "git@ssh.danielhuisman.io:exaltio/exaltio-mandate-frontend.git",
    "keywords": [
        "exaltio",
        "mandate",
        "frontend"
    ],
    "scripts": {
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run lint && yarn run build:parcel",
        "build:parcel": "parcel build public/index.html --public-url / --no-source-maps ",
        "dev": "parcel public/index.html",
        "codegen": "apollo client:codegen --target typescript --endpoint https://mandate.exaltio.nl/api/graphql --outputFlat src/types/generatedTypes.ts",
        "codegen:dev": "apollo client:codegen --target typescript --endpoint http://localhost:5003/graphql --outputFlat src/types/generatedTypes.ts --watch"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.5",
        "@types/helmet": "^0.0.45",
        "@types/qs": "^6.9.1",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-google-recaptcha": "^1.1.1",
        "@types/react-helmet": "^5.0.15",
        "@types/react-router-dom": "^5.1.4",
        "@types/yup": "^0.28.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.17",
        "apollo-upload-client": "^13.0.0",
        "formik": "^2.1.4",
        "graphql": "^14.6.0",
        "i18next": "^19.4.3",
        "i18next-browser-languagedetector": "^4.1.1",
        "ibantools": "^3.0.0",
        "moment": "^2.24.0",
        "qs": "^6.9.3",
        "react": "^16.13.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^16.13.1",
        "react-error-boundary": "^1.2.5",
        "react-google-recaptcha": "^2.0.1",
        "react-helmet": "^6.0.0",
        "react-i18next": "^11.3.5",
        "react-piwik": "^1.8.0",
        "react-router-dom": "^5.1.2",
        "react-router-query-params-hook": "^1.0.4",
        "semantic-ui-react": "^0.88.2",
        "yup": "^0.28.4"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@graphql-codegen/cli": "^1.13.3",
        "@graphql-codegen/fragment-matcher": "^1.13.3",
        "apollo": "^2.27.3",
        "babel-plugin-i18next-extract": "^0.7.2",
        "graphql-tag": "^2.10.3",
        "parcel-bundler": "^1.12.4",
        "postcss-modules": "^2.0.0",
        "rimraf": "^3.0.2",
        "tslint": "^6.1.1",
        "tslint-react": "^5.0.0",
        "typescript": "^3.8.3"
    }
}
