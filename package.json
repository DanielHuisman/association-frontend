{
    "name": "exaltio-mandate-frontend",
    "description": "Frontend for temporary Exaltio mandate system.",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "private": true,
    "author": "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)",
    "repository": "git@ssh.danielhuisman.io:exaltio/exaltio-mandate-frontend.git",
    "keywords": [
        "exaltio",
        "mandate",
        "frontend"
    ],
    "scripts": {
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run lint && yarn run build:parcel",
        "build:parcel": "parcel build public/index.html --public-url / --no-source-maps ",
        "dev": "parcel public/index.html",
        "codegen": "apollo client:codegen --target typescript --endpoint https://mandate.exaltio.nl/api/graphql --outputFlat src/types/generatedTypes.ts",
        "codegen:dev": "apollo client:codegen --target typescript --endpoint http://localhost:5000/graphql --outputFlat src/types/generatedTypes.ts --watch"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@types/helmet": "^0.0.45",
        "@types/qs": "^6.9.0",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/react-google-recaptcha": "^1.1.1",
        "@types/react-helmet": "^5.0.14",
        "@types/react-router-dom": "^5.1.3",
        "@types/yup": "^0.26.27",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-http": "^1.5.16",
        "apollo-upload-client": "^12.1.0",
        "formik": "^2.0.8",
        "graphql": "^14.5.8",
        "i18next": "^19.0.2",
        "i18next-browser-languagedetector": "^4.0.1",
        "ibantools": "^2.2.0",
        "moment": "^2.24.0",
        "qs": "^6.9.1",
        "react": "^16.12.0",
        "react-apollo": "^3.1.3",
        "react-dom": "^16.12.0",
        "react-error-boundary": "^1.2.5",
        "react-google-recaptcha": "^2.0.1",
        "react-helmet": "^5.2.1",
        "react-i18next": "^11.2.7",
        "react-piwik": "^1.8.0",
        "react-router-dom": "^5.1.2",
        "react-router-query-params-hook": "^1.0.4",
        "semantic-ui-react": "^0.88.2",
        "yup": "^0.28.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@graphql-codegen/cli": "^1.8.3",
        "@graphql-codegen/fragment-matcher": "^1.8.3",
        "apollo": "^2.21.2",
        "babel-plugin-i18next-extract": "^0.4.2",
        "graphql-tag": "^2.10.0",
        "parcel-bundler": "^1.12.4",
        "postcss-modules": "^1.4.1",
        "rimraf": "^3.0.0",
        "tslint": "^5.20.1",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.4"
    }
}
